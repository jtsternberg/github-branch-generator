{
	"manifest_version": 3,
	"name": "Git Branch Name Generator",
	"version": "1.0",
	"permissions": ["storage", "activeTab", "scripting"],
	"action": {
	  "default_icon": {
		 "16": "images/icon16.png",
		 "48": "images/icon48.png",
		 "128": "images/icon128.png"
	  }
	},
	"background": {
	  "service_worker": "background.js"
	}
 }